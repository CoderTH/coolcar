<map
  id="map"
  latitude="{{location.latitude}}"
  longitude="{{location.longitude}}"
  scale="{{scale}}"
  setting="{{setting}}"
  enable-enable-overlooking="{{isOverLooking}}"
  enable-3D="{{is3D}}"
  markers="{{markers}}"
>
<cover-view class="icon-container">
  <cover-image  src="{{avatarURL?avatarURL:'/resources/account.png'}}" class="round" bindtap="onMyTripsTap"></cover-image>
  <cover-image 
  src="/resources/my_location.png"
  bindtap="onMyLocationTap"
  ></cover-image>
</cover-view>

<cover-view class="bin-container bg-grey round" bindtap="onScanClicked">
<cover-view >扫码租车</cover-view>
</cover-view>

</map>
